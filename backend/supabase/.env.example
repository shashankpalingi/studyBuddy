# Supabase Configuration
# Copy this file to .env and fill in your actual values

# Supabase Project Settings
# Get these from your Supabase project dashboard at https://supabase.com
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Storage Configuration
SUPABASE_STORAGE_BUCKET=study-buddy-files

# Optional: Database Configuration (if using Supabase DB in future)
SUPABASE_DB_PASSWORD=your_database_password_here

# Environment
NODE_ENV=development

# Frontend Environment Variables (for reference)
# These should be added to your frontend/.env file:
# VITE_SUPABASE_URL=https://your-project-id.supabase.co
# VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
